{
  "required": [
    "prefix",
    "name"
  ],
  "properties": {
    "config": {
      "properties": {
        "prefix": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "name": {
      "enum": [
        "env"
      ],
      "type": "string"
    },
    "prefix": {
      "type": "string"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^(?:[0-9a-zA-Z.\\-_~:]+(?: *[0-9a-zA-Z.\\-_~:])*)?$",
        "type": "string"
      },
      "maxItems": 8,
      "uniqueItems": true,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    }
  },
  "allOf": [
    {
      "if": {
        "properties": {
          "name": {
            "const": "env"
          }
        }
      },
      "then": {
        "properties": {
          "config": {
            "properties": {
              "prefix": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "type": "object"
          }
        }
      }
    }
  ],
  "x-koko-config": {
    "disableValidateEndpoint": false,
    "resourceAPIPath": "vaults"
  }
}